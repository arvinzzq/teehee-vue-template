{
  "name": "{{ name }}",
  "description": "{{ description }}",
  "version": "{{ version }}",
  "author": "{{{ author }}}",
  "license": "{{ license }}",
  "scripts": {
    "dev": "node_modules/.bin/webpack-dev-server -d --config config/webpack.dev.config.js",
    "build": "node_modules/.bin/webpack -p --hide-modules --config config/webpack.prod.config.js",
    "release": "npm run build && npm publish"
  },
  "keywords": [
  {{#each keywords}}
    "{{this}}"{{#if @last}}{{else}},{{/if}}
  {{/each}}
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "vue-loader": "^13.5.0",
    "vue-template-compiler": "^2.5.9",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.5",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "vue": "^2.5.16"
  }
}
